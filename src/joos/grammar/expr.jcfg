PrimaryNoNewArray -> ( Expression )
PrimaryNoNewArray -> ClassInstanceCreationExpression
PrimaryNoNewArray -> FieldAccess
PrimaryNoNewArray -> MethodInvocation
PrimaryNoNewArray -> ArrayAccess

